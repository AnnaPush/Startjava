public class Variables {

	public static void main(String[] args) {
		int coreNumber = 2;
		byte operMemory = 4;
		short systemType = 64;
		long price = 37586;
		float coreClock = 2.23f;
		double weight = 1.90d;
		char modelSign = 'u';
		boolean laptop = true;
	 
	System.out.println(operMemory);
	System.out.println(systemType);
	System.out.println(coreNumber);
	System.out.println(price);
	System.out.println(coreClock);
	System.out.println(weight);
	System.out.println(modelSign);
	System.out.println(laptop);
			
	}
} 